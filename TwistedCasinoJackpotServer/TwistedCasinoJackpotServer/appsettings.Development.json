{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Session": {
    "SecurePolicy": "None",
    "SameSite": "Lax"
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:5001"],
    "AllowedMethods": ["GET", "POST", "PUT", "DELETE"],
    "AllowedHeaders": ["Content-Type"],
    "AllowCredentials": true
  },
  "AllowedHosts": "*"
}
